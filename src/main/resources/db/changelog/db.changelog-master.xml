<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="molimkarbalaei">
        <createTable tableName="charging_station">
            <column name="id" type="VARCHAR">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)"/>
            <column name="power" type="DECIMAL(10,2)"/>
            <column name="latitude" type="DECIMAL(9,6)"/>
            <column name="longitude" type="DECIMAL(9,6)"/>
            <column name="zipcode" type="VARCHAR(20)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
